[HelloPHP]
 Title = Display a string
 Abstract = Display a string which contains an hello message
 processVersion = 1
 storeSupported = true
 statusSupported = true
 serviceProvider = hello.php
 serviceType = PHP
 <MetaData>
   title = Demo
 </MetaData>
 <DataInputs>
  [S]
   Title = the string
   Abstract = The string to add to the hellow one.
   minOccurs = 1
   maxOccurs = 1
   <LiteralData>
    DataType = string
    <Default>
     UOM = meters
    </Default>
    <Supported>
     UOM = feet
    </Supported>
   </LiteralData>
 </DataInputs>
 <DataOutputs>
  [Result]
   Title = The hello string
   Abstract = The Hello message string.
   <LiteralOutput>
    DataType = string
    <Default>
     UOM = meters
    </Default>
    <Supported>
     UOM = feet
    </Supported>
   </LiteralOutput>
 </DataOutputs>  
